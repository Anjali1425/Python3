

#Define a function to check if the first val is greater than 2nd, and return boolean
def is_greater(num1, num2):
    if num1 > num2:
        return True
    else:
        return False

#Define a function to print 'Hello World'

def myfunc():
    print('Hello World')

#Define a function to tke a name arg and print 'Hello Name'

def myfunc(name):
    print('Hello {}'.format(name))

#Define a function to tke a number of arbitary arguments and print their sum

def myfunc(*args):
    return sum(args)
   
#Define a function to tke a number of arbitary arguments and return the even numbrs as a list
def myfunc(*args):
    return [n for n in args if n%2 == 0]

#Define a function to tke a string argument  and return the odd index as lower and even index as upper cases

def myfunc(st):
str=''
for index, l in enumerate(st):
    if index % 2 == 0:
        str+=l.upper()
    else:
        str+=l.lower()
return str
